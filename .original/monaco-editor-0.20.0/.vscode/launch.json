	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "gulp release",
			"program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
			"stopOnEntry": true,
			"args": [
				"release"
			],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "gulp website",
			"program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
			"stopOnEntry": true,
			"args": [
				"website"
			],
			"cwd": "${workspaceFolder}"
		},
	]
}